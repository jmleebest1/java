package collection;

import java.util.HashSet;

public class SetTest {
	public static void main(String[] args) {
		HashSet<String>fruitSet=new HashSet<>();//HashSet은 해쉬코드로 중복 제거
		fruitSet.add("수박");
		fruitSet.add("자두");
		fruitSet.add("망고");
		fruitSet.add("딸기");
		fruitSet.add("사과");//중복검사에 정말 좋다!
		fruitSet.add("사과");
		fruitSet.add("사과");
		
		System.out.println(fruitSet);
		System.out.println(fruitSet.contains("자두"));
		
		Iterator<String>iter=fruitSet.iterator();
		
		//Set은 순서가 없다. toString에 있는 Iterator메소드를 통해 순서를 만들 수 있다.
		
		
		
	}
}
