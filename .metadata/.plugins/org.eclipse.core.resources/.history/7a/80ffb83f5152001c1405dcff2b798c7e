package day05;

class Car{
	String brand;
	String color;
	int price;
	
	void engineStart()	{
		System.out.println("시동 켜기");
	}
	void engineStop()	{
		System.out.println("시동 끄기");
	}
	
}







public class Road {
	public static void main(String[] args) {
		Car momCar = new Car();
		Car dadyCar = new Car();
		Car myCar = new Car();
		
	}
}
