package collection.task;

//학생 한 명이 추가 될 때마다 번호는 자동으로 증가된다.
//아래 작성된 필드의 접근자는 private으로 한다.
public class Student {
	//번호, 이름
	//국어, 영어, 수학 점수
	Student phone;
	Student name;
	
	Student Korean;
	Student English;
	Student Math;
	
	public Student getPhone() {
		return phone;
	}
	public void setPhone(Student phone) {
		this.phone = phone;
	}
	public Student getName() {
		return name;
	}
	public void setName(Student name) {
		this.name = name;
	}
	public Student getKorean() {
		return Korean;
	}
	public void setKorean(Student korean) {
		Korean = korean;
	}
	public Student getEnglish() {
		return English;
	}
	public void setEnglish(Student english) {
		English = english;
	}
	public Student getMath() {
		return Math;
	}
	public void setMath(Student math) {
		Math = math;
	}
	
	
	
	
}
